{
  "name": "react-mobx-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Ready to use boilerplate for react-mobx-typescript apps",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode development --hotOnly",
    "prebuild": "npm run lint",
    "build": "NODE_ENV=production webpack --mode production",
    "lint": "tsc && tslint --project tsconfig.json src/**/*.tsx src/**/*.ts && eslint src/**/*.tsx src/**/*.ts",
    "test": "tslint --fix --project tsconfig.json -c tslint.test.json src/**/*.test.tsx src/**/*.test.ts && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydeep987/react-mobx-typescript-boilerplate.git"
  },
  "keywords": [
    "react",
    "mobx",
    "typescript"
  ],
  "author": "jaydeep987",
  "license": "ISC",
